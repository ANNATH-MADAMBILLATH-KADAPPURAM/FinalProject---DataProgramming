NEW CREATED API

MAIN FUNCTION CODE
exports = function(payload) {
 const mongodb = context.services.get("mongodb-atlas");
 const mycollection = mongodb.db("Cluster0").collection("DevicesUsed");
 return mycollection.find({}).toArray();
 };

